{
  "name": "@umbraco-mcp/umbraco-mcp-cms",
  "version": "0.1.0-alpha.7",
  "type": "module",
  "description": "A model context protocol (MCP) server for Umbraco CMS",
  "main": "index.js",
  "bin": {
    "umbraco": "dist/index.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup --config tsup.config.ts",
    "compile": "tsc --noEmit",
    "watch": "tsup --config tsup.config.ts --watch",
    "generate": "orval --config orval.config.ts",
    "inspect": "npx @modelcontextprotocol/inspector node dist/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "patch-publish-alpha": "npm version prerelease --preid=alpha && npm publish --tag alpha --access public"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Matthew-Wise/umbraco-mcp.git"
  },
  "keywords": [
    "Umbraco",
    "MCP",
    "Model Context Protocol",
    "CMS",
    "MCP-Server",
    "AI"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Matthew-Wise/umbraco-mcp/issues"
  },
  "homepage": "https://github.com/Matthew-Wise/umbraco-mcp#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.4",
    "form-data": "^4.0.4",
    "qs": "^6.14.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/qs": "^6.9.18",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.5.0",
    "glob": "^11.0.2",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "orval": "^7.8.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
